{
    "name": "garethnic/unique",
    "type": "library",
    "description": "A Laravel package to make uuid fields in a database table",
    "keywords": [
        "garethnic",
        "Unique",
        "laravel",
        "uuid"
    ],
    "homepage": "https://github.com/garethnic/unique",
    "license": "MIT",
    "authors": [
        {
            "name": "Gareth Nicholson",
            "email": "gareth.nic@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "ramsey/uuid": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "garethnic\\Unique\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "garethnic\\Unique\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
